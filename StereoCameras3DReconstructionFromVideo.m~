function testAdjustContrust() 
    RGB = imread('./visionData/removeBackground/toCalibrate.png');
    imshow(RGB)
%     RGB2 = imadjust(RGB,[.5 .4 0; .6 .7 1],[]);
%     imshow(RGB2)

    sigma = 0.4;
    alpha = 0.8;
    B = locallapfilt(RGB, sigma, alpha);
    imshowpair(RGB, B, 'montage')
    int =2;

end

